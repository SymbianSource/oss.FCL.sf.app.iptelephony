    {
    buttons =
        {
        CBA_BUTTON 
            {
            id=EAknSoftkeyOptions; 
            txt = text_softkey_option;
            },
        CBA_BUTTON 
            {
            id=EAknSoftkeyExit; 
            txt = text_softkey_exit; 
            },
        CBA_BUTTON 
            {
            id=ECSCCmdOpen; 
            txt = qtn_msk_open;
            }
        };
    }

