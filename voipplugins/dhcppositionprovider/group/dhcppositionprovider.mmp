USERINCLUDE     ../inc

// Default system include paths for middleware layer modules.
APP_LAYER_SYSTEMINCLUDE
SYSTEMINCLUDE     ../../../inc
SYSTEMINCLUDE   /epoc32/include/ecom

LIBRARY     euser.lib
LIBRARY     lbs.lib
LIBRARY     ipapputils.lib
LIBRARY     epospluginfw.lib
LIBRARY     esock.lib
LIBRARY     insock.lib
LIBRARY     commdb.lib

DEBUGLIBRARY flogger.lib

